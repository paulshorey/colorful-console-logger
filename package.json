{
  "name": "colorful-console-logger",
  "version": "0.1.9",
  "description": "Alpha. Experimental. Download and install as local module. Install with `npm link ./path/to/folder`",
  "private": false,
  "author": "Paul Shorey <paulshorey.com>",
  "license": "ISC",
  "dependencies": {},
  "devDependencies": {
    "ava": "^3.15.0",
    "husky": "^4.3.7",
    "parcel": "^1.10.3"
  },
  "scripts": {
    "build": "rm -rf .parcel-cache && parcel build ./src/cconsole*.js",
    "test": "ava --verbose",
    "test:watch": "ava --watch --verbose"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run test"
    }
  },
  "targets": {
    "app": {
      "engines": {
        "browsers": "> 0.75%"
      }
    }
  },
  "type": "commonjs",
  "main": "./dist/index.js",
  "module": "./dist/index.js"
}

{
  "name": "colorful-console-logger",
  "version": "0.1.8",
  "description": "Alpha. Experimental. Download and install as local module. Install with `npm link ./path/to/folder`",
  "private": false,
  "author": "Paul Shorey <paulshorey.com>",
  "license": "ISC",
  "dependencies": {
    "parcel": "^1.10.3"
  },
  "devDependencies": {},
  "scripts": {
    "build": "rm -rf .parcel-cache && parcel build cconsole.js cconsoleInit.js",
    "parcel_v2_build": "npm run before_parcel && rm -rf .parcel-cache && parcel build cconsole.js cconsoleInit.js & npm run before_parcel",
    "parcel_v2_before": "sed -i '' 's/\"main\":/\"xmain\":/' package.json && sed -i '' 's/\"module\":/\"xmodule\":/' package.json",
    "parcel_v2_after": "sed -i '' 's/\"xmain\":/\"main\":/' package.json && sed -i '' 's/\"xmodule\":/\"module\":/' package.json"
  },
  "targets": {
    "app": {
      "engines": {
        "browsers": "> 0.75%"
      }
    }
  },
  "type": "commonjs",
  "main": "index.js",
  "module": "index.mjs",
  "exports": {
    "default": "./index.js",
    "module": "./index.mjs"
  }
}
